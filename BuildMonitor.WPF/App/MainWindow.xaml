<Window x:Class="BuildMonitor.WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="600" Width="800">
    <Grid>
        <Button Name="LoadButton" Content="View raw data" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="-1.427,-1.45" Click="Button_Click"/>
        <Button Name="ReportByDay" Content="Daily report" HorizontalAlignment="Left" Margin="103,10,0,0" VerticalAlignment="Top" Width="75" Click="ReportByDay_Click"/>
        <Button Name="ReportBySolution" Content="Solution report" HorizontalAlignment="Left" Margin="194,10,0,0" VerticalAlignment="Top" Width="102" Click="ReportBySolution_Click"/>
        <DataGrid Name="DataGrid" HorizontalAlignment="Left" Margin="10,35,0,0" VerticalAlignment="Top" Height="524" Width="772"/>
    </Grid>
</Window>
